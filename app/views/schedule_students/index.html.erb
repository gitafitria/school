<p id="notice"><%= notice %></p>

<h1>Schedule Students</h1>

<table>
  <thead>
    <tr>
      <th>Schedule</th>
      <th>Student</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @schedule_students.each do |schedule_student| %>
      <tr>
        <td><%= schedule_student.schedule_id %></td>
        <td><%= schedule_student.student_id %></td>
        <td><%= link_to 'Show', schedule_student %></td>
        <td><%= link_to 'Edit', edit_schedule_student_path(schedule_student) %></td>
        <td><%= link_to 'Destroy', schedule_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule Student', new_schedule_student_path %>
